{
    "name": "madmis/activity-log-bundle",
    "type": "symfony-bundle",
    "description": "Activity log bundle - Extended doctrine loggable bundle",
    "keywords": ["bundle", "doctrine", "loggable", "activity", "symfony"],
    "homepage": "https://github.com/madmis/ActivityLogBundle.git",
    "authors": [
        {
            "name": "Dmitry Machin",
            "email": "madmis@inbox.ru"
        }
    ],
    "license": "MIT",
    "require": {
        "stof/doctrine-extensions-bundle": "^1"
    },
    "autoload": {
        "psr-4": {
            "ActivityLogBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require-dev": {
